    "3I0BTBYZAY7CF3RQBLAYTJTA05JY0A": [
        {
            "record": {
                "recalled": {
                    "1": {
                        "end": 23,
                        "start": 0,
                        "text": "I am a type 2 diabetic."
                    },
                    "2": {
                        "end": 63,
                        "start": 29,
                        "text": "I went in to see my doctor in June"
                    },
                    "3": {
                        "end": 139,
                        "start": 64,
                        "text": "she decided that my A1c was not low enough even though it was in the low 7s"
                    },
                    "4": {
                        "end": 342,
                        "start": 283,
                        "text": "I take something besides insulin to help regulate diabetes."
                    },
                    "5": {
                        "end": 510,
                        "start": 469,
                        "text": "I have been taking Metfornin for 14 years"
                    },
                    "6": {
                        "end": 624,
                        "start": 557,
                        "text": "The doctor increased my dosage of this drug to 3 daily instead of 2"
                    },
                    "7": {
                        "end": 755,
                        "start": 732,
                        "text": "I started getting sick,"
                    },
                    "8": {
                        "end": 860,
                        "start": 806,
                        "text": "I had ran out of blood pressure medicine about 2 weeks"
                    },
                    "9": {
                        "end": 1047,
                        "start": 941,
                        "text": "I also started taking my blood pressure medicine and started having side effects like nausea and dizziness"
                    },
                    "10": {
                        "end": 1228,
                        "start": 1191,
                        "text": "I woke up in the hospital on July 4th"
                    },
                    "11": {
                        "end": 1285,
                        "start": 1231,
                        "text": "My sugar had crashed from taking too much of this drug"
                    },
                    "12": {
                        "end": 1362,
                        "start": 1328,
                        "text": "he rushed me to the emergency room"
                    },
                    "13": {
                        "end": 1413,
                        "start": 1363,
                        "text": " I woke up after having been given a glucose drip."
                    }
                },
                "retold": {
                    "14": {
                        "end": 157,
                        "start": 62,
                        "text": "My doctor who thought my A1c level was too high instructed me to increase one of my medications"
                    },
                    "15": {
                        "end": 347,
                        "start": 291,
                        "text": "I had been taking this prescribed medicine for 13 years."
                    },
                    "16": {
                        "end": 377,
                        "start": 348,
                        "text": "I had gotten up to go to work"
                    },
                    "17": {
                        "end": 446,
                        "start": 415,
                        "text": "I tested and it was indeed low,"
                    },
                    "19": {
                        "end": 650,
                        "start": 637,
                        "text": "I went to bed"
                    },
                    "20": {
                        "end": 786,
                        "start": 730,
                        "text": " I woke up in the hospital in a bed with an IV in my arm"
                    },
                    "21": {
                        "end": 1030,
                        "start": 901,
                        "text": "I got up a while later and was acting screwy-if a person hasn't seen a hypoglycemic episode they might think the person was drunk"
                    },
                    "22": {
                        "end": 1074,
                        "start": 1040,
                        "text": "I got up then fell in the bathroom"
                    },
                    "23": {
                        "end": 1111,
                        "start": 1076,
                        "text": "my husband came in and talked to me"
                    },
                    "24": {
                        "end": 1267,
                        "start": 1235,
                        "text": "He finally got me up and dressed"
                    },
                    "25": {
                        "end": 1303,
                        "start": 1274,
                        "text": "he rushed me to the hospital "
                    }
                }
            },
