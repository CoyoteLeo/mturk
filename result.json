    "3XUHV3NRVLKPOFN2YRCMOEUYH6HH5J": [
        {
            "record": {
                "recalled": {
                    "2": {
                        "end": 62,
                        "start": 0,
                        "text": "I went on a canoe float last weekend with my husband and kids."
                    },
                    "3": {
                        "end": 242,
                        "start": 228,
                        "text": " He went along"
                    },
                    "4": {
                        "end": 354,
                        "start": 277,
                        "text": "My dad is a lot of fun and made the bus ride to the drop off point memorable."
                    },
                    "5": {
                        "end": 387,
                        "start": 355,
                        "text": "Everyone on the bus was laughing"
                    },
                    "6": {
                        "end": 472,
                        "start": 412,
                        "text": "People were saying that they wanted to float with our group "
                    },
                    "7": {
                        "end": 565,
                        "start": 515,
                        "text": "people would see us and yell out to him \"Grandpa!\""
                    },
                    "8": {
                        "end": 589,
                        "start": 566,
                        "text": "Everyone remembered him"
                    },
                    "9": {
                        "end": 686,
                        "start": 645,
                        "text": "He would say something funny back to them"
                    },
                    "10": {
                        "end": 996,
                        "start": 966,
                        "text": "I don't remember a float trip."
                    },
                    "11": {
                        "end": 1030,
                        "start": 1013,
                        "text": "My kids got quiet"
                    },
                    "12": {
                        "end": 1079,
                        "start": 1032,
                        "text": "Everyone realized how bad his memory has become"
                    },
                    "26": {
                        "end": 915,
                        "start": 836,
                        "text": "we were reminiscing about all of the fun things that happened on the float trip"
                    }
                },
                "retold": {
                    "13": {
                        "end": 55,
                        "start": 0,
                        "text": "We went on a float trip with my dad and my kids grandpa"
                    },
                    "14": {
                        "end": 230,
                        "start": 117,
                        "text": "The ride to the drop off point was a lot of fun and many people on the bus were enjoying my dad's sense of humor."
                    },
                    "15": {
                        "end": 264,
                        "start": 231,
                        "text": "Everyone was calling him grandpa."
                    },
                    "16": {
                        "end": 342,
                        "start": 265,
                        "text": "People were saying that they wanted to float along with us on the float trip."
                    },
                    "17": {
                        "end": 419,
                        "start": 344,
                        "text": "ome parts of the float were a little hard when the water was moving swiftly"
                    },
                    "18": {
                        "end": 457,
                        "start": 424,
                        "text": "we got caught up in downed trees,"
                    },
                    "19": {
                        "end": 578,
                        "start": 532,
                        "text": "people from the bus would yell out \"Grandpa!!\""
                    },
                    "20": {
                        "end": 632,
                        "start": 583,
                        "text": "my dad would wave to them and say something funny"
                    },
                    "22": {
                        "end": 1036,
                        "start": 956,
                        "text": "He does have a hard time remembering things or following conversations sometimes"
                    },
                    "23": {
                        "end": 1208,
                        "start": 1091,
                        "text": "He was having a lot of fun and was also really good at steering the canoe and telling the person in front how to help"
                    },
                    "24": {
                        "end": 1342,
                        "start": 1246,
                        "text": "we were talking about how much fun we had and discussing some of the funny stories from the trip"
                    },
                    "25": {
                        "end": 1510,
                        "start": 1442,
                        "text": "He was very much in shock when we were telling him what had happened"
                    },
                    "27": {
                        "end": 1655,
                        "start": 1558,
                        "text": "it especially hit my kids really hard because they really didn't know how ugly Alzheimer's can be"
                    }
                }
            },
